let str = "Full stack development";
let vowelCount = 0;
for (let i = 0; i < str.length; i++) {
  let char = str[i].toLowerCase();
  if (char === 'a' || char === 'e' || char === 'i' || char === 'o' || char === 'u') {
    vowelCount++;
  }
}
console.log("print the number of vowels in a string:", vowelCount);
